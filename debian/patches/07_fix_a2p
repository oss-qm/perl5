+		    $$ = oper3(OASSIGN,$2,$1,$3);
+		    if ((ops[$1].ival & 255) == OFLD)
+			lval_field = TRUE;
+		    else if ((ops[$1].ival & 255) == OVFLD)
+			lval_field = TRUE;
 		}
 	;
 
